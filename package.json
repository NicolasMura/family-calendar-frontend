{
  "name": "family-calendar",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --disable-host-check",
    "start:sw": "ng serve --configuration production --disable-host-check --proxy-config proxy.conf.json --verbose",
    "start:sw-a-tester": "ng serve --configuration production --optimization=false --progress=true --disable-host-check --proxy-config proxy.conf.json --sourceMap=true --live-reload --watch",
    "build": "ng build",
    "build:demo": "ng build --configuration demo --base-href ./",
    "build:prod": "ng build --configuration production --base-href ./",
    "serve": "http-server -p 8080 -c-1 dist/public",
    "build-and-serve": "yarn build && yarn serve",
    "deploy:test": "ng build --base-href ./ && ./deploy.sh",
    "deploy:prod": "yarn build:prod && ./deploy.sh",
    "deploy:prod:demo": "yarn build:demo && ./deploy.sh -d",
    "deploy": "./deploy.sh",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular-material-components/datetime-picker": "^4.0.5",
    "@angular-material-components/moment-adapter": "^4.0.1",
    "@angular/animations": "^11.0.2",
    "@angular/cdk": "^11.0.1",
    "@angular/common": "^11.0.2",
    "@angular/compiler": "^11.0.2",
    "@angular/core": "^11.0.2",
    "@angular/forms": "^11.0.2",
    "@angular/material": "^11.0.1",
    "@angular/material-moment-adapter": "^11.0.1",
    "@angular/platform-browser": "^11.0.2",
    "@angular/platform-browser-dynamic": "^11.0.2",
    "@angular/router": "^11.0.2",
    "@angular/service-worker": "^11.0.2",
    "hammerjs": "^2.0.8",
    "jwt-decode": "^3.1.2",
    "moment": "^2.29.1",
    "ngx-webstorage": "^6.0.0",
    "roboto-fontface": "^0.10.0",
    "rxjs": "^6.6.3",
    "swiper": "^6.4.11",
    "tslib": "^2.0.3",
    "zone.js": "^0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1100.2",
    "@angular/cli": "^11.0.2",
    "@angular/compiler-cli": "^11.0.2",
    "@types/jasmine": "^3.6.2",
    "@types/node": "^14.14.10",
    "codelyzer": "^6.0.1",
    "jasmine-core": "^3.6.0",
    "jasmine-spec-reporter": "^6.0.0",
    "karma": "^5.2.3",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "ng-packagr": "^11.0.3",
    "protractor": "^7.0.0",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3",
    "typescript": "4.0.5"
  }
}
